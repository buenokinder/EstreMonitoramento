    <style>
        @media screen {
            header.onlyprint, footer.onlyprint {
                display: none; /* Hide from screen */
            }
        }

        @media print {

            * {
                background: transparent !important;
                color: #000 !important;
                text-shadow: none !important;
                filter: none !important;
                -ms-filter: none !important;
            }



            header.onlyprint {
                position: fixed; /* Display only on print page (each) */
                top: -10; /* Because it's header */
                float: right;
            }

            header img.logo {
                float: right;
            }

            footer.onlyprint {
                position: fixed;
                bottom: 0; /* Because it's footer */
            }

            .no-print, nav, footer, video, audio, object, embed {
                display: none;
            }

            .print {
                display: block;
            }


            img {
                max-width: 100%;
            }

            aside {
                display: block;
                page-break-before: always;
            }

            .page-break {
                page-break-before: always;
            }

            /*Estilos da Demo*/
            .header.print h1 {
                width: 100%;
                margin-bottom: 0.5cm;
                font-size: 18pt;
            }

            .header.print:after {
                content: "";
            }

            h1 {
                color: #000;
                background: none;
            }

            thead {
                display: table-header-group;
            }

            nav, aside {
                display: none;
            }

            h1::before {
                @top-right {
                    content: "Pagina " counter(chapter) ": ";
                }
            }

            body, article {
                width: 100%;
                margin: 0;
                padding: 0;
            }

            body {
                display: block;
                page: main;
                counter-reset: page 1;
            }

            h1 {
                page-break-before: right;
                prince-page-group: start;
                string-set: chapter content();
                counter-increment: chapter;
                counter-reset: section;
            }

           .table-large {
                    background: #fff;
                    margin-bottom: 1.25rem;
                    border: 1px solid #ddd;
                    table-layout: fixed;
                    width: 98%;
                    margin-left: 1%;
                    margin-right: 1%;
            }

            .table-large tr th {
                font-size: 8pt !important;
                font-weight: normal !important;
            }

            .table-large tr td {
                font-size: 6pt !important;
                font-weight: normal !important;
            }

            table tr:nth-of-type(even) {
                background: #f9f9f9;
            }

            th.green {
                background-color: green !important;
                text-align: center;
                font-color: white !important;
                -webkit-print-color-adjust: exact;
            }

            tr.lightgreen {
                background-color: #9ACD32 !important;
                -webkit-print-color-adjust: exact;
            }

            tr.base {
                text-align: center;
                font-color: black !important;
                -webkit-print-color-adjust: exact;
                background-color: #f5f5f5 !important;
            }

            th.lightgreen {
                background-color: #9ACD32 !important;
                text-align: center;
                font-color: white !important;
                -webkit-print-color-adjust: exact;
                font-size: 7pt !important;
                font-family: arial;
                font-weight: normal !important;
            }

            
            thead.gray {
                background-color: #f5f5f5 !important;
                -webkit-print-color-adjust: exact;
            }

            tr.gray {
                background-color: #f5f5f5 !important;
                -webkit-print-color-adjust: exact;
            }

            th.yellow {
                background-color: yellow !important;
                text-align: center;
                -webkit-print-color-adjust: exact;
                font-color: white !important;
            }

            th.orange {
                background-color: orange !important;
                text-align: center;
                -webkit-print-color-adjust: exact;
                font-color: white !important;
            }

            th.red {
                background-color: red !important;
                text-align: center;
                -webkit-print-color-adjust: exact;
                font-color: white !important;
            }

            th.black {
                background-color: black !important;
                text-align: center;
                -webkit-print-color-adjust: exact;
                color: white !important;
            }

            @page {
                size: A4;
                font-family: Times, Serif;
                font-size: 10pt;
                text-align: justify;
                margin-top: 2cm;
                margin-bottom: 2cm;
                margin-inside: 1.5cm;
                margin-outside: 1.5cm;
            }

            @page :left {
                margin-left: 1cm;
            }

            @page :right {
                margin-left: 1cm;
            }


            @page :first {
            }

            @page :blank {
                @top-center {
                    content: "This page is intentionally left blank.";
                }
            }


            h1 {
                page-break-before: always;
            }


            @page {
                counter-increment: page;
                counter-reset: page 1;

                @top-right {
                    content: "Page " counter(page) " of " counter(pages);
                }
            }
        }
    </style>
</head>


