    <script type="text/javascript">

		function padLeftZero(value){
			return parseInt(value)<10?"0"+value.toString():value.toString();
		}

        function fecharModal(modal){
          $('#'+modal+'').closeModal();
        };

        function getDate(value){
            var data=null;

            if(value.toString().indexOf("/")>=0){
                var ret = value.split('/');
                data  = new Date(ret[2], parseInt(ret[1]-1), ret[0]);
            }else{
               data = new Date(value);
            }
            var ano = data.getFullYear();
            var mes = data.getMonth() + 1;
            var dia = data.getDate();

            if(isNaN(dia) || isNaN(mes) || isNaN(ano)){
                return value;
            }

            var retorno = dia + "/" + mes + "/" + ano;
            return retorno;
        }

		function getDateQuery(value){
            var data=null;

            if(value.toString().indexOf("/")>=0){
                var ret = value.split('/');
                data  = new Date(ret[2], parseInt(ret[1]-1), ret[0]);
            }else{
               data = new Date(value);
            }
            var ano = data.getFullYear();
            var mes = data.getMonth() + 1;
            var dia = data.getDate();

            if(isNaN(dia) || isNaN(mes) || isNaN(ano)){
                return value;
            }

            var retorno = ano + "-" + mes + "-" + dia;
            return retorno;
        }

		function getDateTimeString(value){
            var data = getDateTime(value);
            var ano = data.getFullYear();
            var mes = data.getMonth() + 1;
            var dia = data.getDate();
			var hora = data.getHours();
			var minuto = data.getMinutes()

            if(isNaN(dia) || isNaN(mes) || isNaN(ano)){
                return value;
            }

			var retorno = "";
			if(parseInt(hora)==0 && parseInt(minuto)==0){
				retorno = dia + "/" + mes + "/" + ano;
			}else{
				retorno = padLeftZero(dia) + "/" + padLeftZero(mes) + "/" + ano + " " + padLeftZero(hora) + ":" + padLeftZero(minuto);
			}

            return retorno;
        }

		function getDateTimeStringQuery(value){
            var data = getDateTime(value);
            var ano = data.getFullYear();
            var mes = data.getMonth() + 1;
            var dia = data.getDate();
			var hora = data.getHours();
			var minuto = data.getMinutes()

            if(isNaN(dia) || isNaN(mes) || isNaN(ano)){
                return value;
            }

			var retorno = "";
			if(parseInt(hora)==0 && parseInt(minuto)==0){
				retorno = ano + "-" + mes + "-" + dia;
			}else{
				retorno = ano + "-" + padLeftZero(mes) + "-" + padLeftZero(dia) + " " + padLeftZero(hora) + ":" + padLeftZero(minuto);
			}

            return retorno;
        }

		function getDateTime(value){
            var data=null;

            if(value.toString().indexOf("/")>=0){
				var ret = value.split("/");
				var mes = parseInt(ret[1]) - 1;
				var dia = ret[0];
				ret = ret[2].split(' ');
				var ano = ret[0];
				var hora = 0;
				var minuto = 0;
				var possuiHoras =  ret.length>1;

				if(possuiHoras){
					ret = ret[1].split(':')
					hora = ret[0];
					minuto = ret[1];
				}
				data = new Date(ano, mes, dia, hora, minuto);
            }else{
               data = new Date(value);
            }

			return data;
        }

		$(function(){
			$('.datepicker').bootstrapMaterialDatePicker({ format: 'DD/MM/YYYY' });
			$('.datetimepicker').bootstrapMaterialDatePicker({ format: 'DD/MM/YYYY HH:mm' });
		});
    </script>

