    <script src="/js/dependencies/angular-route.min.js"></script>
    <script src="/js/private/dashboard/angular-leaflet-directive.min.js"></script>
    <script src="/js/private/dashboard/angular-xeditable/js/xeditable.js"></script>
    <script src="/js/private/dashboard/ui-bootstrap-tpls.min.js"></script>
    <script src="/js/private/dashboard/angular-upload.min.js"></script>

    <script src="/js/private/dashboard/App.js"></script>
    <script src="/js/private/dashboard/MonitoramentoAterroController.js"></script>

    <script src="/js/private/dashboard/Route.js"></script>


    <script type="text/javascript" src="/js/private/dashboard/material.min.js"></script>
