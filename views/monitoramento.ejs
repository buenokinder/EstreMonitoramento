<body id="layouts-horizontal" ng-app="DashboardModule" ng-controller="MonitoramentoAterroController">
    <div>
        <div id="loader-wrapper">
            <div id="loader"></div>
            <div class="loader-section section-left"></div>
            <div class="loader-section section-right"></div>
        </div>

        <div id="main">
            <div class="wrapper">
                <section id="content">

                    <div class="container">

                        <div class="section">

                            <div class="row">
                                <div class="col s12 m12">
                                    <h2 id="nomeaterro" class="ng-binding" style="text-align:center">{{dashboard.current.aterro.nome}}</h2>
                                </div>
                            </div>

                            <div class="row" id="doismapas" style="display:none">
                                <div class="col s6 m6 l6 parallax-container">
                                    <div class="row">
                                        <p style="text-align:center">
                                            Horizontal
                                        </p>
                                    </div>

                                    <iframe src="{{dashboard.current.mapaHorizontal}}" style="border:none;width:100%;height:100%" class="maph"></iframe>
                                </div>

                                <div class="col s6 m6 l6 parallax-container">

                                    <div class="row">
                                        <p style="text-align:center">
                                            Vertical
                                        </p>
                                    </div>

                                    <iframe src="{{dashboard.current.mapaVertical}}" style="border:none;width:100%;height:100%" class="mapv"></iframe>
                                </div>
                            </div>

                            <!--<div class="row" ng-show="{{data.exibirMapaHorizontal}} && !{{data.exibirMapaVertical}}">-->
                            <div class="row" id="mapahorizontal" style="display:none">
                                <div class="col s12 m12 parallax-container">
                                    <div class="row">
                                        <p style="text-align:center">
                                            Horizontal
                                        </p>
                                    </div>

                                    <iframe src="{{dashboard.current.mapaHorizontal}}" style="border:none;width:100%;height:100%" class="maph"></iframe>
                                </div>

                            </div>

                            <!--<div class="row" ng-show="!{{data.exibirMapaHorizontal}} && {{data.exibirMapaVertical}}">-->
                            <div class="row" id="mapavertical" style="display:none">
                                <div class="col s12 m12 parallax-container">
                                    <div class="row">
                                        <p style="text-align:center">
                                            Vertical
                                        </p>
                                    </div>

                                    <iframe src="{{dashboard.current.mapaVertical}}" style="border:none;width:100%;height:100%" class="mapv"></iframe>
                                </div>

                            </div>

                            <!--<div class="row" ng-show="{{data.exibirLegenda}}" style="margin-top: 35px;">-->
                            <div class="row" id="legenda" style="margin-top: 35px;display:none">
                                <table>
                                    <tr>
                                        <td style="background-color:green"></td>
                                        <td>Velocidade menor do que 0,25</td>
                                        <td style="background-color:yellow"></td>
                                        <td>Velocidade menor do que 1</td>
                                        <td style="background-color:orange"></td>
                                        <td>Velocidade menor do que 4</td>
                                        <td style="background-color:red"></td>
                                        <td>Velocidade menor do que 14</td>
                                        <td style="background-color:black"></td>
                                        <td>Velocidade maior do que 14</td>
                                    </tr>
                                </table>
                            </div>

                            <!--<div class="row" ng-show="{{dashboard.config.exibirfatorseguranca}}">-->
                            <div class="row" id="fator" style="display:none;margin-top:20px">
                                <div class="col s12 m12 l6">
                                    FATOR SEGURAN&Ccedil;A

                                    <div class="row">
                                        <img id="preview" />
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </section>

            </div>
        </div>
    </div>

